/* pages/more/more.wxss */
Page {
  background-color: #8ce48c;
}
.mainView {
  min-height: 1000px;
  padding: 2%;
  font-size: 50rpx;
  font-weight: 600;
}
.function-item {
  height: 200rpx;
  margin-top: 5%;
  border-radius: 10px;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #FFFFFF;
}
.my-icon {
  display: inline;
}
.my-text {
  display: inline;
  margin-left: 5%;
}
.focusOn {
  background-color: #e25e3c;
  color: #FFFFFF;
}
.viewEnd {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 5% 0%;
  border-radius: 10rpx;
  background-color: #8ce48c;
  font-size: 40rpx;
}